{
  "manifest_version": 2,
  "name": "children wall",
  "description": "children",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "wall for children",
    "default_icon": "icons/wall.png"
  },
  "permissions": [
    "activeTab",
    "tabs", "http://*/*", "https://*/*"
  ]
}